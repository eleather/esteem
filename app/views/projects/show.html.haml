#scaffold-deprecated
  %p#notice= notice

  %p
    %b Name:
    = @project.name
  %p
    %b Slug:
    = @project.slug
  %p
    %b Description:
    = @project.description
  %p
    %b Organization:
    = link_to @project.organization.name, @project.organization

  = link_to 'Edit', edit_project_path(@project)
  \|
  = link_to 'Back', projects_path

#radials
  - if @radials.any?
    = link_to 'Learn More', radials_project_path(@project)
  #data
    = render :partial => 'radials/radial_score', :collection => @radials, :as => :radial
  #graph
  
#questions

#suggestions
  = render :partial => 'suggestions/new'
  = render @suggestions
